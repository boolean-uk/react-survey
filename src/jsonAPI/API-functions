const fetchSurveyResponses = () => {
  let fetchedData;
  fetch("http://localhost:3000/responses")
    .then((res) => {
      if (!res.ok) {
        throw new Error(`Network response was not ok, status: ${res.status}`);
      }
      return res.json();
    })
    .then((data) => {
      console.log(data);
      return data;
    })
    .catch((error) => {
      console.error("Fetch error:", error);
    });
};

export default fetchSurveyResponses;
